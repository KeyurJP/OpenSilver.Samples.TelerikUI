    <Style x:Key="RadGalleryStyle" TargetType="telerik1:RadGallery">
        <Setter Property="ItemWidth" Value="156"/>
        <Setter Property="ViewportWidth" Value="156"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:RadWrapPanel/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
