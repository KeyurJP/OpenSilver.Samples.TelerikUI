    <Style x:Key="RibbonWindow_TitleStyle" TargetType="telerikRibbonViewPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsHitTestVisible" Value="false"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Margin" Value="0 5"/>
        <Setter Property="IsActive" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonViewPrimitives:WindowTitle">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock x:Name="Title" Text="{TemplateBinding Title}" Foreground="{StaticResource MainBrush}"/>
                        <TextBlock x:Name="Divider"
                                Text="{telerik:LocalizableResource Key=RibbonBarWindowTitleDivider}"
                                Visibility="{TemplateBinding DividerVisibility}"
                                Foreground="{StaticResource MainBrush}"/>
                        <TextBlock x:Name="ApplicationName" Text="{TemplateBinding ApplicationName}" Foreground="{StaticResource MainBrush}"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRibbonWindowButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadButton">
                    <Grid Background="{TemplateBinding Background}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="Content"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="Content"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.5" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="Content"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <ContentPresenter x:Name="Content"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Content="{TemplateBinding Content}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Margin="{TemplateBinding Padding}"
                                Opacity="0.7"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRibbonWindowToggleButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid x:Name="ContentGrid" Background="{TemplateBinding Background}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="RestoreIndicator"/>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="CollapseIndicator"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="RestoreIndicator"/>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="CollapseIndicator"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.5" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="RestoreIndicator"/>
                                        <DoubleAnimation Duration="0" To="0.5" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="CollapseIndicator"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="CheckStates">
                                <VisualState x:Name="Checked">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Restore" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Collapse" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unchecked">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Restore" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Collapse" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Indeterminate"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <StackPanel x:Name="Restore" Width="12" Height="10">
                            <ToolTipService.ToolTip>
                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowMaximize}"/>
                            </ToolTipService.ToolTip>
                            <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
                                <Rectangle x:Name="RestoreIndicator" Stroke="{StaticResource MainBrush}" StrokeThickness="2" Width="10" Height="10" Opacity="0.7"/>
                            </Grid>
                        </StackPanel>
                        <Grid Visibility="Collapsed" x:Name="Collapse" VerticalAlignment="Center" HorizontalAlignment="Center" Background="Transparent">
                            <ToolTipService.ToolTip>
                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowRestoreDown}"/>
                            </ToolTipService.ToolTip>
                            <Path x:Name="CollapseIndicator" Fill="{StaticResource MainBrush}" Data="M0,0 1,1 9,1 9,9 10,9 10,0 z M0,2 0,10 8,10 8,2z" Opacity="0.7"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerik1:RadRibbonWindow" x:Key="RadRibbonWindowStyle">
        <Setter Property="TitleBarStyle" Value="{StaticResource RibbonWindow_TitleStyle}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="WindowBackground" Value="{StaticResource AccentBrush}"/>
        <Setter Property="Background" Value="{StaticResource MainBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource BasicBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadRibbonWindow">
                    <Grid Background="Transparent">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="WindowStates">
                                <VisualState x:Name="Maximized">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="bottomBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="rightBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="leftBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="outerBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="topRect" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonPanel" Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0 0 0 3"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="bottomBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="rightBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="leftBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="border" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="outerBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="topRect" Storyboard.TargetProperty="Visibility" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="buttonPanel" Storyboard.TargetProperty="Margin" Duration="0">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="0 0 7 3"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid Margin="-1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>

                            <!-- window background -->
                            <Border x:Name="outerBorder"
                                    Grid.ColumnSpan="3"
                                    Grid.RowSpan="3"
                                    Background="{TemplateBinding WindowBackground}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"/>

                            <!-- top border -->
                            <Border x:Name="border" Grid.ColumnSpan="3" Height="30" Background="{StaticResource RibbonWindow_Resize_Background}"/>
                            <Border Grid.RowSpan="3" Grid.Column="1" Background="{TemplateBinding WindowBackground}" BorderThickness="{TemplateBinding BorderThickness}"/>

                            <!-- top resize rectangle -->
                            <Rectangle x:Name="topRect"
                                    telerik1:RadRibbonWindow.ResizeEdge="Top"
                                    Cursor="SizeNS"
                                    Canvas.ZIndex="2"
                                    Margin="20 0 8 0"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Stretch"
                                    Grid.ColumnSpan="3"
                                    Height="5"
                                    Fill="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- top move rectangle -->
                            <Rectangle x:Name="topInnerRect" Margin="40 0 0 4" Canvas.ZIndex="1" Grid.Column="1" Fill="{StaticResource RibbonWindow_Resize_Background}"/>
                            <StackPanel x:Name="buttonPanel" Canvas.ZIndex="1" Orientation="Horizontal" Grid.ColumnSpan="3" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 7 3">
                                <telerik:RadButton x:Name="minimizeButton" Command="telerik:WindowCommands.Minimize" Style="{StaticResource RadRibbonWindowButtonStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowMinimize}"/>
                                    </ToolTipService.ToolTip>
                                    <Grid Width="10" Height="10">
                                        <Rectangle Fill="{StaticResource MainBrush}" VerticalAlignment="Bottom" Height="2"/>
                                    </Grid>
                                </telerik:RadButton>
                                <telerik:RadToggleButton x:Name="maximizeButton" IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=WindowState, Converter={StaticResource BooleanToWindowStateConverter}, Mode=TwoWay}" Style="{StaticResource RadRibbonWindowToggleButtonStyle}"/>
                                <telerik:RadButton x:Name="closeButton" Command="telerik:WindowCommands.Close" Style="{StaticResource RadRibbonWindowButtonStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowClose}"/>
                                    </ToolTipService.ToolTip>
                                    <Path Stroke="{StaticResource MainBrush}" StrokeThickness="2" Width="10" Height="10" Stretch="Fill" Data="M0,10 10,0 M0,0 10,10"/>
                                </telerik:RadButton>
                            </StackPanel>

                            <!-- left resize border -->
                            <Border x:Name="leftBorder"
                                    telerik1:RadRibbonWindow.ResizeEdge="Left"
                                    Cursor="SizeWE"
                                    Grid.Row="1"
                                    Width="7"
                                    Background="{StaticResource RibbonWindow_Resize_Background}"
                                    Margin="0 20"/>

                            <!-- right resize border -->
                            <Border x:Name="rightBorder"
                                    Cursor="SizeWE"
                                    telerik1:RadRibbonWindow.ResizeEdge="Right"
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    Width="7"
                                    Background="{StaticResource RibbonWindow_Resize_Background}"
                                    Margin="0 20"/>

                            <!-- bottom resize border -->
                            <Border x:Name="bottomBorder"
                                    Cursor="SizeNS"
                                    telerik1:RadRibbonWindow.ResizeEdge="Bottom"
                                    Grid.Row="2"
                                    Grid.ColumnSpan="3"
                                    Height="7"
                                    VerticalAlignment="Bottom"
                                    Background="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- top left resize rectangle -->
                            <Rectangle x:Name="topLeftRect"
                                    Cursor="SizeNWSE"
                                    Canvas.ZIndex="0"
                                    telerik1:RadRibbonWindow.ResizeEdge="TopLeft"
                                    Grid.ColumnSpan="2"
                                    Grid.RowSpan="2"
                                    Width="30"
                                    Height="30"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Left"
                                    Fill="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- left bottom resize rectangle -->
                            <Rectangle x:Name="bottomLeftRect"
                                    Cursor="SizeNESW"
                                    telerik1:RadRibbonWindow.ResizeEdge="BottomLeft"
                                    Grid.ColumnSpan="2"
                                    Grid.RowSpan="2"
                                    Grid.Row="1"
                                    Width="20"
                                    Height="20"
                                    VerticalAlignment="Bottom"
                                    HorizontalAlignment="Left"
                                    Fill="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- top right resize rectangle -->
                            <Rectangle x:Name="topRightRect"
                                    Canvas.ZIndex="0"
                                    telerik1:RadRibbonWindow.ResizeEdge="TopRight"
                                    Cursor="SizeNESW"
                                    Grid.Column="1"
                                    Grid.ColumnSpan="2"
                                    Grid.RowSpan="2"
                                    Width="30"
                                    Height="30"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Right"
                                    Fill="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- right bottom resize rectangle -->
                            <Rectangle x:Name="bottomRightRect"
                                    Cursor="SizeNWSE"
                                    telerik1:RadRibbonWindow.ResizeEdge="BottomRight"
                                    Grid.ColumnSpan="2"
                                    Grid.RowSpan="2"
                                    Grid.Column="1"
                                    Grid.Row="1"
                                    Width="20"
                                    Height="20"
                                    VerticalAlignment="Bottom"
                                    HorizontalAlignment="Right"
                                    Fill="{StaticResource RibbonWindow_Resize_Background}"/>

                            <!-- content -->
                            <Border Background="{TemplateBinding Background}" Grid.Column="1" Grid.Row="1"/>
                            <ContentPresenter Grid.Column="1" Grid.RowSpan="2" Canvas.ZIndex="0" Margin="0 2 0 0"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--<Style TargetType="telerik1:RadRibbonWindow" BasedOn="{StaticResource RadRibbonWindowStyle}"/>-->

</ResourceDictionary>
