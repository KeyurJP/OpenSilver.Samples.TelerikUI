    <!--<Style TargetType="telerik1:QuickAccessToolBar" BasedOn="{StaticResource QuickAccessToolBarStyle}"/>
    <Style TargetType="telerikRibbonViewPrimitives:WindowTitle" BasedOn="{StaticResource WindowTitleStyle}"/>
    <Style TargetType="telerik1:RadRibbonView" BasedOn="{StaticResource RadRibbonViewStyle}"/>
