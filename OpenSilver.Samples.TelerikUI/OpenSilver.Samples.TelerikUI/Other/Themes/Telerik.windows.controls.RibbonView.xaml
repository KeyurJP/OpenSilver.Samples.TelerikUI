                            Padding="0"
                            Margin="-1 -1 -1 0"
                            Background="{StaticResource MainBrush}">
                        <Path HorizontalAlignment="Center" VerticalAlignment="Center" Fill="{StaticResource StrongBrush}" Data="M3,0 L6,4 L0,4z"/>
                    </telerik1:RadRibbonButton>
                    <telerik1:RadRibbonButton x:Name="ScrollDownButton"
