                    <telerik1:RadRibbonDropDownButton x:Name="PopupButton"
                            telerik1:ScreenTip.Title="{TemplateBinding MoreButtonTitle}"
                            HorizontalAlignment="Stretch"
                            DropDownIndicatorVisibility="Collapsed"
                            VerticalContentAlignment="Center"
                            CornerRadius="0 0 1 0"
                            Visibility="{TemplateBinding PopupLaunchButtonVisibility}"
                            Grid.Column="1"
                            Grid.Row="2"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Padding="0"
                            Margin="-1 0 -1 0"
                            Background="{StaticResource MainBrush}">
                        <Path
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Stretch="None"
                                Fill="{StaticResource StrongBrush}"
                                Data="M0,2 L6,2 L3,6z M0,0 L6,0 L6,1 L0,1 z"/>
                    </telerik1:RadRibbonDropDownButton>
                </Grid>
            </Border>
            <telerik1:RadRibbonDropDownButton x:Name="CollapsedButton" Size="Large" LargeImage="{TemplateBinding Icon}" Text="{TemplateBinding Title}" Visibility="Collapsed">
                <telerik1:RadRibbonDropDownButton.DropDownContent>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer
                                Padding="0"
                                HorizontalScrollBarVisibility="Hidden"
                                IsTabStop="False"
                                Height="{TemplateBinding PopupViewportHeight}"
                                VerticalScrollBarVisibility="Visible"
                                Margin="0 0 -1 -2"
                                BorderThickness="0 0 0 1">
                            <ContentPresenter x:Name="PopupHost" Width="{TemplateBinding PopupViewportWidth}"/>
                        </ScrollViewer>
                        <ItemsControl
                                HorizontalContentAlignment="Stretch"
                                Grid.Row="1"
                                ItemsSource="{TemplateBinding PopupMenuItems}"
                                Foreground="{TemplateBinding Foreground}"
                                BorderThickness="0 1 0 0"
                                Margin="0 2 0 0"/>
                    </Grid>
                </telerik1:RadRibbonDropDownButton.DropDownContent>
            </telerik1:RadRibbonDropDownButton>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadRibbonGalleryStyle" TargetType="telerik1:RadRibbonGallery">
